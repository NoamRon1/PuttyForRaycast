{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "putty",
  "title": "Putty",
  "description": "Search through Putty saved connection",
  "icon": "extension-icon.png",
  "license": "MIT",
  "commands": [
    {
      "name": "search-connections",
      "title": "Search PuTTY Connections",
      "description": "Search through saved PuTTY sessions",
      "mode": "view",
      "subtitle": "Search saved PuTTY sessions"
    },
    {
      "name": "add-connection",
      "title": "Add PuTTY Connection",
      "description": "Create and save a new PuTTY session",
      "mode": "view",
      "subtitle": "Create a new PuTTY session"
    }
  ],
  "preferences": [
    {
      "name": "puttyPath",
      "title": "PuTTY Executable Path",
      "type": "textfield",
      "placeholder": "C:\\Program Files\\PuTTY\\putty.exe",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.0",
    "@raycast/utils": "^2.2.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "dev": "ray develop",
    "lint": "ray lint",
    "fix-lint": "ray lint --fix",
    "build": "ray build",
    "publish": "npx @raycast/api@latest publish",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1"
  },
  "author": "noam_ron",
  "owner": "stroke-com-3-0",
  "platforms": [
    "Windows"
  ],
  "categories": [
    "Developer Tools",
    "Security"
  ]
}